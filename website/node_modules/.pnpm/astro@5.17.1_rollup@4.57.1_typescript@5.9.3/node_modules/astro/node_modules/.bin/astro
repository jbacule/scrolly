#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jobacx/projects/portfolios/scrolly/website/node_modules/.pnpm/astro@5.17.1_rollup@4.57.1_typescript@5.9.3/node_modules/astro/node_modules:/Users/jobacx/projects/portfolios/scrolly/website/node_modules/.pnpm/astro@5.17.1_rollup@4.57.1_typescript@5.9.3/node_modules:/Users/jobacx/projects/portfolios/scrolly/website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jobacx/projects/portfolios/scrolly/website/node_modules/.pnpm/astro@5.17.1_rollup@4.57.1_typescript@5.9.3/node_modules/astro/node_modules:/Users/jobacx/projects/portfolios/scrolly/website/node_modules/.pnpm/astro@5.17.1_rollup@4.57.1_typescript@5.9.3/node_modules:/Users/jobacx/projects/portfolios/scrolly/website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
